package main;

import java.util.List;

public class Bank {

	public List<String> transactions;
	public Bank() {
		solde = 0;
	}
	private double solde;

	public double getSolde() {
		return solde;
	}

	public void depot(double d) {
		if( d > 0 )
			solde += d;
	}

	public void retrait(double sub) {
		if( sub > 0 )
			solde -= sub;
		
	}

	public List<String> getTransactions() {
		return null;
	}


}
